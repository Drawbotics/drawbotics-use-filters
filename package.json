  },
  "devDependencies": {
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "eslint": "^6.7.2",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-jest": "^23.1.1",
    "history": "^4.10.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3"
